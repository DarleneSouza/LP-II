package geometria;

public class Retangulo {
	private double altura;
	private double base;
	public void setAltura(double altura) {
		if(altura > 0) {
			this.altura = altura;
		}
	}public void setBase(double base) {
		if(base > 0) {
			this.base = base;
		}
	}
		public double calcularArea() {
			double area = altura * base;
			return area;
			
		}
			
		}
	

