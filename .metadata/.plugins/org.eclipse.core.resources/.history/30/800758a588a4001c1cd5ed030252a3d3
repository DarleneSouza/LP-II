import java.util.Scanner;

public class Main { 
	public static void main(String[] args) {
		//método de entrada por onde começa a  executar
		
		Scanner leitor = new Scanner(System.in);
		// classe  que permite fazer leitura do que é digitado
		
		String nome = leitor.nextLine();
		//faz leitura no formato de texto  e retorna para próxima linha o que foi digitado
	}	
	
}
