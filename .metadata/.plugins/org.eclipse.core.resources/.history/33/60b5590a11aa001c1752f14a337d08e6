package geometria;

public class Main {

	public static void main(String[] args) {


		
		Triangulo primeiroTriangulo = new Triangulo();
		primeiroTriangulo.setAltura(2);
		primeiroTriangulo.setBase(4);
		
		double calcularArea = primeiroTriangulo.calcularAreaTriangulo();
		System.out.println("Area do triangulo: " + calcularArea);
		
	}

}
